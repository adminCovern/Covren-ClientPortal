{
  "name": "covren-client-portal",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "npx tailwindcss -i ./styles.css -o ./dist/styles.css --minify && npx babel index.jsx --out-dir dist --presets=@babel/preset-env,@babel/preset-react && cp index.html dist/ && cp favicon.io dist/favicon.ico",
    "dev": "npx tailwindcss -i ./styles.css -o ./dist/styles.css --watch & npx babel index.jsx --out-dir dist --presets=@babel/preset-env,@babel/preset-react --watch",
    "test": "jest",
    "start": "npx serve dist -s"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.7",
    "@babel/plugin-transform-modules-umd": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-react": "^7.24.7",
    "@testing-library/react": "^16.0.0",
    "autoprefixer": "^10.4.19",
    "jest": "^29.7.0",
    "postcss": "^8.4.39",
    "serve": "^14.2.1",
    "tailwindcss": "^3.4.4"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.45.0",
    "bcryptjs": "^3.0.2",
    "compression": "^1.7.4",
    "connect-pg-simple": "^10.0.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.18.2",
    "express-validator": "^7.2.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "pg": "^8.16.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": "iife",
          "targets": {
            "browsers": [
              "last 2 versions",
              "> 0.5%",
              "not dead"
            ]
          },
          "exclude": [
            "@supabase/supabase-js"
          ]
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-transform-modules-umd"
    ]
  },
  "jest": {
    "testEnvironment": "jsdom"
  }
}
